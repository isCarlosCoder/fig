struct Point {
    x
    y

    fn init(x, y) {
        this.x = x
        this.y = y
    }

    fn move(dx, dy) {
        this.x = this.x + dx
        this.y = this.y + dy
    }
}

let p = Point(10, 20)
print(p.x)
print(p.y)

p.move(5, 10)
print(p.x)
print(p.y)

print("---")

